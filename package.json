{
  "name": "theholgaproject",
  "version": "2.0.0",
  "description": "Drag & drop an image into your browser and apply a Holga-like filter.",
  "main": "src/js/index.js",
  "scripts": {
    "copy": "./bin/copy-files.sh",
    "dev:js": "webpack",
    "watch": "nodemon -e css,js -x \"npm run dev:js\"",
    "serve": "http-server -p 9090 build/ -c-1",
    "prestart": "npm run copy -- build && npm run dev:js",
    "start": "npm run watch & npm run serve"
  },
  "repository": "https://github.com/jonchretien/theholgaproject",
  "author": "Jon Chretien",
  "license": "",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "http-server": "^0.11.1",
    "nodemon": "^1.17.5",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8"
  }
}
